                  </xs:sequence>
                  <xs:attribute type="xs:date" name="Diaareportar" use="required">
                    <xs:annotation>
                      <xs:documentation  xml:lang="es-mx">
                        Atributo requerido para expresar el día del periodo a reportar
                      </xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute type="t_Permiso" name="Numero" use="required">
              <xs:annotation>
                <xs:documentation  xml:lang="es-mx">
                  Atributo requerido para expresar el número de permiso otorgado por la Comisión Reguladora de Energía (CRE) que presenta el reporte
                </xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute type="xs:date" name="FechaInicio" use="required">
              <xs:annotation>
                <xs:documentation  xml:lang="es-mx">
                  Atributo requerido para expresar la fecha de inicio del reporte
                </xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute type="xs:date" name="FechaFin" use="required">
              <xs:annotation>
                <xs:documentation  xml:lang="es-mx">
                  Atributo requerido para expresar la fecha final del reporte
                </xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute type="xs:int" name="TipoReporte" use="required">
              <xs:annotation>
                <xs:documentation  xml:lang="es-mx">
                  Identificador único del tipo de reporte REST del catálogo: https://api-catalogo.cre.gob.mx/api/utiles/ObtenerTiposReporte Donde: ----- tipoReporteId: Identificador único (entero) ------ Nombre: Nombre (cadena) NOTA: ------ Recomendamos consumir este servicio en línea. Si almacena este catálogo dentro de alguna solución, sugerimos se actualice constantemente.
                </xs:documentation>
              </xs:annotation>
            </xs:attribute>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="t_Permiso">
    <xs:restriction base="xs:string">
      <xs:minLength value="15"/>
      <xs:maxLength value="24"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[H]{1}/[0-9]{4,6}/[COM]{3}/[0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_permiso_TRA">
    <xs:restriction base="xs:string">
      <xs:minLength value="14"/>
      <xs:maxLength value="24"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[PL]{2}[/]{1}[0-9]{1,6}[/]{1}[TRA|DIS]{3}[/]{1}[OM|DUC|TM]{2,3}[/]{1}[0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_permiso_ALM">
    <xs:restriction base="xs:string">
      <xs:minLength value="14"/>
      <xs:maxLength value="24"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[PL]{2}[/]{1}[0-9]{1,6}[/]{1}[ALM]{3}[/]{1}([AE]{2}[/]{1})?[0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_permisoCRECliente">
    <xs:restriction base="xs:string">
      <xs:minLength value="14"/>
      <xs:maxLength value="24"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[PL|H]{1,2}[/]{1}[0-9]{1,6}[/]{1}[EXP|DIS|COM]{3}[/]{1}([ES|ESA|AE|OM]{2,3}[/]{1})?([MM]{2}[/]{1})?[0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_permiso_COM">
    <xs:restriction base="xs:string">
      <xs:minLength value="15"/>
      <xs:maxLength value="24"/>
      <xs:whiteSpace value="collapse"/>
      <!-- Patron original del valor de abajo -> "[H]{1}/[0-9]{4,6}/[COM]{3}/[0-9]{4}" -->
      <xs:pattern value="(H/[0-9]{4,6}/COM/[0-9]{4})|(PL/[0-9]{1,6}/(TRA|DIS)/(OM|DUC|TM)/[0-9]{4})"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_permiso_DIS">
    <xs:restriction base="xs:string">
      <xs:minLength value="15"/>
      <xs:maxLength value="24"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[PL]{2}[/]{1}[0-9]{1,6}[/]{1}[TRA|DIS]{3}[/]{1}[OM|DUC|TM]{2,3}[/]{1}[0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_fecha">
    <xs:restriction base="xs:string">
      <xs:pattern value="(0[1-9]|[12][0-9]|3[01])/(0[1-9]|1[012])/(19|20)\d\d"/>
      <xs:minLength value="10"/>
      <xs:maxLength value="10"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_productoId">
    <xs:restriction base="xs:int">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_SubproductoId">
    <xs:restriction base="xs:int">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_precio">
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:minInclusive value="0.01"/>
      <xs:maxInclusive value="99999.99"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_flete">
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:minInclusive value="0.01"/>
      <xs:maxInclusive value="999999999.99"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_volumen">
    <xs:annotation>
      <xs:documentation>Atributo requerido para expresar volumenes</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:minInclusive value="0.01"/>
      <xs:maxInclusive value="99999999999.99"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_OtroTipoDescuento">
    <xs:annotation>
      <xs:documentation>
        Atributo requerido para especificar en caso de que el descuento sea otro
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="50"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[A-Z0-9]{3,200}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_razonSocial">
    <xs:annotation>
      <xs:documentation>Tipo definido para la razón social asociada</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="3"/>
      <xs:maxLength value="200"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[A-Z0-9Ñ&amp;\s]{3,200}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_RFC">
    <xs:annotation>
      <xs:documentation>
        Tipo definido para expresar clave del Registro Federal de Contribuyente
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="12"/>
      <xs:maxLength value="13"/>
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="[A-ZÑ&amp;]{3,4}[0-9]{2}[0-1][0-9][0-3][0-9][A-Z0-9]?[A-Z0-9]?[0-9A-Z]?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_inventario">
    <xs:annotation>
      <xs:documentation>Tipo definido para expresar inventarios</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:totalDigits value="13"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_PermisoSENER">
    <xs:restriction base="xs:string">
      <xs:minLength value="18"/>
      <xs:maxLength value="30"/>
      <xs:whiteSpace value="collapse"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>